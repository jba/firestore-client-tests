
description: "nested paths"
update: <
  doc_ref_path: "projects/projectID/databases/(default)/documents/C/d"
  json_data: "{\"a\": 1, \"b\": {\"c\": 2}}"
  request: <
    database: "projects/projectID/databases/(default)"
    writes: <
      update: <
        name: "projects/projectID/databases/(default)/documents/C/d"
        fields: <
          key: "a"
          value: <
            integer_value: 1
          >
        >
        fields: <
          key: "b"
          value: <
            map_value: <
              fields: <
                key: "c"
                value: <
                  integer_value: 2
                >
              >
            >
          >
        >
      >
      update_mask: <
        field_paths: "a"
        field_paths: "b"
      >
      current_document: <
        exists: true
      >
    >
  >
>
